AT_BANNER([DATA LIST])

AT_SETUP([DATA LIST LIST with empty fields])
AT_DATA([data-list.pspp], [dnl
DATA LIST LIST NOTABLE /A B C (F1.0).
BEGIN DATA.
,,
,,3
,2,
,2,3
1,,
1,,3
1,2,
1,2,3
END DATA.

LIST.
])
